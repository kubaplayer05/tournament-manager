{
  "env": {
    "browser": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "quotes": [
      "warn",
      "double"
    ],
    "indent": [
      "warn",
      4
    ],
    "semi": [
      "warn",
      "always"
    ]
  }
}
